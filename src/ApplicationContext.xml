<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="emp1" class="SpringDIwithConstructor.Employee">
		<constructor-arg value="101" />
		<constructor-arg value="shital" />
		<constructor-arg value="shital@gmail.com" />
	</bean>

	<bean id="emp2" class="SpringDIwithConstructor.Employee">
		<constructor-arg value="102" />
		<constructor-arg value="shubham" />
		<constructor-arg value="shubham@gmail.com" />
	</bean>

	<bean id="companybean" class="SpringDIwithConstructor.Company">
		<constructor-arg value="10" />
		<constructor-arg value="FortuneCloud" />
		<constructor-arg>
			<list>
				<value>HealthCare</value>
				<value>Hotel mgmt</value>
				<value>Banking Mgmt</value>
			</list>
		</constructor-arg>
		<constructor-arg>
			<set>
				<value>Pune</value>
				<value>Mumbai</value>
				<value>Bangalore</value>
			</set>
		</constructor-arg>

		<constructor-arg>
			<map>
				<entry key="101" value-ref="emp1" />
				<entry key="102" value-ref="emp2" />
			</map>
		</constructor-arg>
	</bean>

</beans>
